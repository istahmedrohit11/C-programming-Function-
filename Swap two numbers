#include<stdio.h>
void swap(int a,int b);
void swap(int a,int b){
    printf("Before swapping: a=%d, b=%d \n",a,b);
    int temp=a;
    a=b;
    b=temp;
      printf("after swapping: a=%d, b=%d \n",a,b);
}
int main(){
    int a,b;
    printf(" Enter first number: ");
    scanf("%d",&a);
     printf(" Enter second number: ");
    scanf("%d",&b);
    swap(a,b);
    return 0;
}

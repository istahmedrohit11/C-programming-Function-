#include<stdio.h>
void max_min(int a,int b);
void max_min(int a,int b){
    if(a>b){
        printf("Maximum %d \n",a);
        printf("Minimum %d \n",b);
    }
    else{
        printf("Maximum %d \n",b);
        printf("Minimum %d \n",a);
    }
}
#include<stdio.h>
int main(){
    int a,b;
    printf("Enter any number : ");
    scanf("%d",&a);
    printf("Enter any number : ");
    scanf("%d",&b);
    max_min(a,b);
    return 0;
}
